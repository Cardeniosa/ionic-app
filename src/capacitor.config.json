{
  "appId": "com.emergya.geteduroam",
  "appName": "geteduroam",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "webDir": "www",
  "cordova": {},
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 3000,
      "launchAutoHide": true,
      "androidSplashResourceName": "Splash",
      "androidScaleType": "FIT_CENTER",
      "androidSpinnerStyle": "large",
      "iosSpinnerStyle": "large",
      "spinnerColor": "#4269b5",
      "showSpinner": true
    }
  }
}
